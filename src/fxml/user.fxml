<AnchorPane prefHeight="627.0" prefWidth="989.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UserController">
   <children>
      <TableView fx:id="menuTableView" layoutX="31.0" layoutY="73.0" prefHeight="312.0" prefWidth="452.0">
        <columns>
            <TableColumn fx:id="menuIdTableColumn" prefWidth="118.0" text="菜品编号" />
          <TableColumn fx:id="menuNameTableColumn" prefWidth="235.0" text="菜品" />
