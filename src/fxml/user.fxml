      <Button fx:id="removeButton" disable="true" layoutX="623.0" layoutY="473.0" mnemonicParsing="false" onMouseClicked="#onMouseClickedRemoveButton" text="移除" />
      <Button fx:id="payButton" disable="true" layoutX="745.0" layoutY="473.0"  onMouseClicked="#onMouseClickedPayButton"  mnemonicParsing="false" text="结算" />
   </children>
</AnchorPane>
