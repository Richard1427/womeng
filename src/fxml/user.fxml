        </columns>
      </TableView>
      <Button fx:id="addChartButton" disable="true" layoutX="214.0" layoutY="473.0" mnemonicParsing="false" onMouseClicked="#onMouseClickedAddChartButton" text="添加到购物车" />
      <TableView fx:id="shopTableView" layoutX="506.0" layoutY="73.0" prefHeight="312.0" prefWidth="452.0">
